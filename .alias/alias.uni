# uni-aliases

alias alias.uni='vim ~/.alias/alias.uni && source ~/.alias/alias.uni'

newuni() {
	echo "alias $1=\"${@:2}\"" >> ~/.alias/alias.uni
	source ~/.alias/alias.uni
}

